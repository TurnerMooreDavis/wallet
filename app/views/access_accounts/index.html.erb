<p id="notice"><%= notice %></p>

<h1>Listing Access Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @access_accounts.each do |access_account| %>
      <tr>
        <td><%= access_account.name %></td>
        <td><%= access_account.amount %></td>
        <td><%= link_to 'Show', access_account %></td>
        <td><%= link_to 'Edit', edit_access_account_path(access_account) %></td>
        <td><%= link_to 'Destroy', access_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Access account', new_access_account_path %>
